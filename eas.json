{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "bash eas-build-pre-install.sh"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "prebuildCommand": "bash eas-build-pre-install.sh"
    },
    "default": {
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "bash eas-build-pre-install.sh"
    }
  },
  "submit": {
    "production": {}
  }
}

